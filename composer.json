{
    "name": "melmccann/integration-framework-bundle",
    "description": "Smartbox Framework Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Smartbox\\Integration\\FrameworkBundle\\": ""
      }
    },
    "require": {
        "php": ">=5.6.0",
        "ext-pcntl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/symfony": "^2.7.0",
        "doctrine/annotations": "^1.2.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "^2.4.0",
        "sensio/distribution-bundle": "^4.0.0",
        "sensio/framework-extra-bundle": "^3.0.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/serializer-bundle": "^0.13.0",
        "ocramius/proxy-manager": "~1.0",
        "stomp-php/stomp-php": "4.*",
        "doctrine/doctrine-bundle": "^1.6",
        "melmccann/besimple-soap": "@dev",
        "melmccann/core-bundle": "@dev"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "~4.0",
        "phpunit/phpunit-mock-objects": "~3.2",
        "phpunit/phpunit": "~5.4",
        "sensio/generator-bundle": "~2.3",
        "mongodb/mongodb": "~1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/melmccann/BeSimpleSoap"
        },
        {
            "type": "vcs",
            "url": "https://github.com/melmccann/core-bundle"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true
}
